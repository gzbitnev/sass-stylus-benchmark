div
    position fixed
    text-align center
    font 14px / 18px
    color rgba(0,0,0,0.5)
    background-color hsl(160, 100%, 85%)
    background hsl(160, 100%, 85%) url('logo.png') no-repeat left top / 48px 48px
    background-clip content-box
    display grid
    grid-template-columns auto 100px 1fr 2fr
    grid-template-rows 50px 5em min-content auto
    transform translateX(1px)
    transition height 1s ease-in-out
    content ''
    box-shadow 4px 4px 32px rgba(0,0,0,0.1), 0 0 8px #222222
    overflow hidden

